<odoo>
<data noupdate="1">
    <record
            id="conference_chair_attribute_pillow_attribute_line"
            model="product.template.attribute.line"
        >
        <field
                name="product_tmpl_id"
                ref="product.product_product_11_product_template"
            />
        <field name="attribute_id" ref="product_attribute_seat_pillow" />
        <field
                name="value_ids"
                eval="[
            (6, 0, [
                    ref('product_attribute_value_seat_pillow_1'),
                    ref('product_attribute_value_seat_pillow_2'),
                    ref('product_attribute_value_seat_pillow_3'),
                    ref('product_attribute_value_seat_pillow_4'),
                ]
            )
        ]"
            />
    </record>

    <record
            id="conference_chair_attribute_wheels_attribute_line"
            model="product.template.attribute.line"
        >
        <field
                name="product_tmpl_id"
                ref="product.product_product_11_product_template"
            />
        <field name="attribute_id" ref="product_attribute_wheels" />
        <field
                name="value_ids"
                eval="[
            (6, 0, [
                    ref('product_attribute_value_wheels_1'),
                    ref('product_attribute_value_wheels_2'),
                    ref('product_attribute_value_wheels_3'),
                    ref('product_attribute_value_wheels_4'),
                ]
            )
        ]"
            />
    </record>

    <record
            id="conference_chair_attribute_backrest_attribute_line"
            model="product.template.attribute.line"
        >
        <field
                name="product_tmpl_id"
                ref="product.product_product_11_product_template"
            />
        <field name="attribute_id" ref="product_attribute_backrest" />
        <field
                name="value_ids"
                eval="[
            (6, 0, [
                    ref('product_attribute_value_backrest_1'),
                    ref('product_attribute_value_backrest_2'),
                ]
            )
        ]"
            />
    </record>
</data>
</odoo>
